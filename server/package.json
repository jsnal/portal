{
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=dev nodemon src/app.js --exec babel-node --presets babel-preset-env",
    "dev:lint": "NODE_ENV=dev nodemon src/app.js --exec 'npm run lint && babel-node' --presets babel-preset-env",
    "start": "NODE_ENV=production node build/app.js",
    "build": "babel -d ./build ./src -s",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.4.1",
    "highlight.js": "^10.4.1",
    "linkify-it": "^3.0.2",
    "markdown-it": "^10.0.0",
    "mongoose": "^5.9.12",
    "morgan": "^1.10.0",
    "unpack-content": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.3"
  }
}
