{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "no-underscore-dangle": [0],
      "no-plusplus": [0],
      "no-cond-assign": [0]
    }
}
